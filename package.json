{
  "name": "mikro-orm-entity-factory",
  "version": "0.2.0",
  "description": "A library for generating mikro-orm entities in bulk.",
  "main": "lib/index.js",
  "typings": "lib/index",
  "homepage": "https://github.com/pragmaticivan/mikro-orm-entity-factory",
  "repository": {
    "type": "git",
    "url": "https://github.com/pragmaticivan/mikro-orm-entity-factory"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test": "jest --ci --runInBand",
    "test:ci": "ENVIRONMENT=ci jest --ci --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,ts",
    "format": "eslint . --ext .js,ts --fix"
  },
  "keywords": [
    "mikroorm",
    "testing",
    "typescript",
    "seeding",
    "jest",
    "factory",
    "seeder",
    "faker"
  ],
  "license": "ISC",
  "files": [
    "lib"
  ],
  "dependencies": {
    "faker": "5.4.0",
    "@mikro-orm/core": "^4.4.3",
    "@types/faker": "5.1.7"
  },
  "devDependencies": {
    "@mikro-orm/postgresql": "^4.4.3",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.31",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "eslint": "7.20.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "5.0.9",
    "jest": "26.6.3",
    "jest-junit": "12.0.0",
    "lint-staged": "10.5.4",
    "mysql": "2.18.1",
    "pg": "8.5.1",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "ts-jest": "26.5.1",
    "ts-node": "9.1.1",
    "typescript": "4.1.5",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "pretty-quick --staged",
      "npm run lint:fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./test-results/coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/src/tests/test-utils",
      "<rootDir>/examples/*"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  }
}
